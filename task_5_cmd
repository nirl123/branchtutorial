git remote add origin git@github.com:nirl123/branchtutorial.git
git remote -v
git fetch --all
git branch -r
git checkout h2
git branch -vv
git branch new-branch
git add task3.txt
git status 
git commit -m "first commit"
git push origin new-branch
git checkout master
git add file4.txt
git status
git commit -m "commit file4"

